{
  "name": "nodeapp",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpileOnly server.ts",
    "prod": "tsc && node ./build/server.js",
    "start": "nodemon start",
    "test-dev": "npm test",
    "test:e2e": "node e2e",
    "test:unit": "mocha tests/**/*.test.js",
    "test": "yarn run test:unit && yarn run test:e2e"
  },
  "dependencies": {
    "@types/express": "^4.17.6",
    "@types/express-serve-static-core": "^4.17.8",
    "@types/node": "^14.0.22",
    "@types/validator": "^13.1.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "graphql": "^14.5.8",
    "http-errors": "~1.6.3",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.22.3",
    "sequelize-cli": "^5.5.1",
    "ts-node": "^8.10.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "^4.2.8",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "chai": "^4.2.0",
    "concurrently": "^5.0.2",
    "jest": "^26.0.1",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.2",
    "ts-jest": "^26.1.0"
  },
  "_moduleAliases": {
    "@root": ".",
    "@models": "src/models",
    "@config": "src/config",
    "@controllers": "src/controllers",
    "@routes": "src/routes",
    "@modules": "src/modules",
    "@services": "src/services"
  }
}
